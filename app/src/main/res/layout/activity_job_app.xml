<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".JobApp" android:background="#FFFFFF">

    <TextView
            android:text="Job Application"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/correo"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.849" app:layout_constraintVertical_bias="0.0"
            android:textColor="#000000" android:textSize="34sp" android:textStyle="bold"/>
    <ImageView
            android:layout_width="109dp"
            android:layout_height="66dp" app:srcCompat="@drawable/mal" android:id="@+id/imageView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.013"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:layout_width="186dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:text="ex:name@gmail.com"
            android:ems="10"
            android:id="@+id/email"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.693" app:layout_constraintVertical_bias="0.601"
            android:textColor="#000000" android:textSize="18sp"/>
    <TextView
            android:layout_width="129dp"
            android:layout_height="49dp"
            android:inputType="textPersonName"
            android:text="First Name"
            android:ems="10"
            android:id="@+id/firstName"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.137" app:layout_constraintHorizontal_bias="0.546"
            android:textColor="#000000" android:editable="false" android:textSize="18sp"/>
    <TextView
            android:text="Email Address"
            android:layout_width="76dp"
            android:layout_height="46dp" android:id="@+id/textView5"
            android:textColor="#000000" android:textSize="18sp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.101"
            app:layout_constraintVertical_bias="0.585" android:textAlignment="inherit"/>
    <TextView
            android:layout_width="119dp"
            android:layout_height="57dp"
            android:inputType="textPersonName"
            android:text="Postal/Zip "
            android:ems="10"
            android:id="@+id/postal"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.511" app:layout_constraintHorizontal_bias="0.534"
            android:textColor="#000000" android:textSize="18sp"/>
    <TextView
            android:layout_width="122dp"
            android:layout_height="59dp"
            android:inputType="textPersonName"
            android:text="State/Province"
            android:ems="10"
            android:id="@+id/state"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.979" app:layout_constraintVertical_bias="0.404"
            android:textColor="#000000" android:textSize="18sp"/>
    <TextView
            android:layout_width="128dp"
            android:layout_height="49dp"
            android:inputType="textPersonName"
            android:text="Position"
            android:ems="10"
            android:id="@+id/position"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.561" app:layout_constraintVertical_bias="0.824"
            android:textColor="#000000" android:textSize="18sp"/>
    <TextView
            android:layout_width="115dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:text="Last Name"
            android:ems="10"
            android:id="@+id/lastName"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.979" app:layout_constraintVertical_bias="0.139"
            android:textColor="#000000" android:textSize="16sp"/>
    <TextView
            android:text="Phone number"
            android:layout_width="92dp"
            android:layout_height="41dp" android:id="@+id/textView7"
            android:textColor="#000000" android:textSize="18sp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintVertical_bias="0.691" android:textAlignment="inherit"/>
    <TextView
            android:text="Current Address"
            android:layout_width="70dp"
            android:layout_height="47dp" android:id="@+id/textView6"
            android:textColor="#000000" android:textSize="20sp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.085"
            app:layout_constraintVertical_bias="0.235" android:textAlignment="inherit"/>
    <TextView
            android:layout_width="186dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:text="Street Address"
            android:ems="10"
            android:id="@+id/street"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.688"
            app:layout_constraintVertical_bias="0.238" android:textColor="#000000" android:textSize="18sp"/>
    <TextView
            android:layout_width="113dp"
            android:layout_height="56dp"
            android:inputType="textPersonName"
            android:text="Area Code"
            android:ems="10"
            android:id="@+id/areaCode"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.706" app:layout_constraintHorizontal_bias="0.52"
            android:textColor="#000000" android:textSize="18sp"/>
    <TextView
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:inputType="textPersonName"
            android:text="Date"
            android:ems="10"
            android:id="@+id/date"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.593" app:layout_constraintVertical_bias="0.884"
            android:textColor="#000000" android:textSize="18sp"/>
    <TextView
            android:text="Start date"
            android:layout_width="79dp"
            android:layout_height="27dp" android:id="@+id/textView8"
            android:textColor="#000000" android:textSize="18sp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.087"
            app:layout_constraintVertical_bias="0.864"/>
    <TextView
            android:text="Applying for position"
            android:layout_width="118dp"
            android:layout_height="44dp" android:id="@+id/textView9"
            android:textColor="#000000" android:textSize="18sp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.098"
            app:layout_constraintVertical_bias="0.802" android:textAlignment="inherit"/>
    <TextView
            android:layout_width="124dp"
            android:layout_height="60dp"
            android:inputType="textPersonName"
            android:text="City"
            android:ems="10"
            android:id="@+id/city"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.536"
            android:textColor="#000000" app:layout_constraintVertical_bias="0.405" android:textSize="18sp"/>
    <TextView
            android:layout_width="186dp"
            android:layout_height="42dp"
            android:inputType="textPersonName"
            android:text="Street Address Line 2"
            android:ems="10"
            android:id="@+id/street2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.688" app:layout_constraintVertical_bias="0.314"
            android:textColor="#000000" android:textSize="18sp"/>
    <TextView
            android:layout_width="112dp"
            android:layout_height="56dp"
            android:inputType="textPersonName"
            android:text="Phone number"
            android:ems="10"
            android:id="@+id/phone"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.946" app:layout_constraintVertical_bias="0.706"
            android:textColor="#000000" android:textSize="18sp"/>
    <TextView
            android:text="Full name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            android:textColor="#000000" android:textSize="20sp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.08"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.129"/>
    <TextView
            android:layout_width="123dp"
            android:layout_height="56dp"
            android:inputType="textPersonName"
            android:text="Coutry"
            android:ems="10"
            android:id="@+id/country"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.968" app:layout_constraintVertical_bias="0.511"
            android:textColor="#000000" android:textSize="18sp"/>
    <Button
            android:text="Regresar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnBack"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.976"
            android:background="#1A0D0D" android:backgroundTint="#073eb5"/>
</androidx.constraintlayout.widget.ConstraintLayout>